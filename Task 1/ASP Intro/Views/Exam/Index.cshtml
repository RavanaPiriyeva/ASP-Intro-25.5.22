@using ASP_Intro.Models;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
   <h1 class="m-4 text-center text-warning">Exams</h1>
    <table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Date</th>
    </tr>
  </thead>
  <tbody>
       @foreach (Exam item in ViewBag.Exam)
        {

      
       <tr>
           
      <th scope="row">@item.Id</th>
      <td> <a href="/Exam/Detail/@item.Id" target="_blank" class="text-decoration-none text-warning"> @item.ExamName </a></td>
      <td>@item.Date.ToString("MMMM dd, yyyy")</td>
      
     
    </tr>
    
        }
   

  </tbody>
</table>
</body>
</html>